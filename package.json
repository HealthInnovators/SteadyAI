{
  "name": "steady-ai-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "test:integration": "NODE_ENV=test DATABASE_URL=$TEST_DATABASE_URL DIRECT_URL=${TEST_DIRECT_URL:-$TEST_DATABASE_URL} node --import tsx --test test/integration/api.integration.test.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/multipart": "^9.0.3",
    "@prisma/client": "6.19.2",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "prisma": "6.19.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
